<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Font?>

<AnchorPane onKeyPressed="#onKeyPressedButton" prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.ViewController">
   <children>
      <Button focusTraversable="false" layoutX="196.0" mnemonicParsing="false" onAction="#onActionButtonUp" text="UP">
         <font>
            <Font name="Arial Black" size="11.0" />
         </font>
      </Button>
      <Button focusTraversable="false" layoutX="179.0" layoutY="388.0" mnemonicParsing="false" onAction="#onActionButtonDown" text="DOWN">
         <font>
            <Font name="Arial Black" size="11.0" />
         </font>
      </Button>
      <Button focusTraversable="false" layoutY="189.0" mnemonicParsing="false" onAction="#onActionButtonLeft" text="LEFT">
         <font>
            <Font name="Arial Black" size="11.0" />
         </font>
      </Button>
      <Button focusTraversable="false" layoutX="369.0" layoutY="201.0" mnemonicParsing="false" onAction="#onActionButtonRight" text="RIGHT">
         <font>
            <Font name="Arial Black" size="11.0" />
         </font>
      </Button>
      <Circle fx:id="circle" fill="DODGERBLUE" focusTraversable="true" layoutX="212.0" layoutY="201.0" radius="80.0" stroke="BLACK" strokeType="INSIDE" />
      <Button layoutX="390.0" layoutY="403.0" mnemonicParsing="false" onAction="#onActionButtonDont" prefHeight="4.0" prefWidth="39.0" text="DONT">
         <font>
            <Font name="Arial Black" size="8.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
